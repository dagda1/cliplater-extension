{
  manifest_version: 2,
  name: "Clip Later",
  description: "Clip crap for later",
  version: "0.0.1",
  author: "Paul Cowan",
  permissions: [
    "tabs",
    "<all_urls>"
  ],
  web_accessible_resources: [
    "js/*"
  ],
  background: {
    scripts: [
      "js/compiled/goog/base.js",
      "js/compiled/cliplater.js"
    ]
  }
}